    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },    
    "source": {
        "include": [
            "object-scrubber.js"
        ]
    },
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "./docs/jsdoc/",
        "recurse": true,
        "tutorials": "./docs/tutorials"
    },
    "templates": {
        "default": {
            "outputSourceFiles": false,
            "includeDate": false
        },
        "cleverLinks": false,
        "monospaceLinks": false
    }
}